{
  "name": "client",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "npm run build:ssr && npm run serve:ssr",
    "dev:start": "ng run client:dev-serve",
    "build": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "build:server:dev": "ng run client:server:development",
    "serve:ssr:dev": "node dist/client/server/main.js",
    "dev:ssr": "ng build && ng run client:server & nodemon --watch dist/client/browser --exec \"node dist/client/browser/main.js\"",
    "test": "ng test",
    "build:server": "ng run client:server:production",
    "build:ssr": "npm run build && npm run build:server && npm run rename:server",
    "rename:server": "node -e \"const fs=require('fs');fs.renameSync('dist/client/server/main.js','dist/client/server/main.cjs')\"",
    "serve:ssr": "node dist/client/server/main.cjs",
    "prerender": "ng run client:prerender",
    "serve:ssr:client": "node dist/client/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.0.3",
    "@angular/common": "^20.0.3",
    "@angular/compiler": "^20.0.3",
    "@angular/core": "^20.2.4",
    "@angular/forms": "^20.0.3",
    "@angular/material": "^20.0.3",
    "@angular/platform-browser": "^20.0.3",
    "@angular/platform-server": "^20.2.4",
    "@angular/router": "^20.0.3",
    "@angular/ssr": "^20.2.2",
    "@tailwindcss/postcss": "^4.1.8",
    "express": "^5.1.0",
    "postcss": "^8.5.4",
    "rxjs": "^7.8.0",
    "tailwindcss": "^4.1.8",
    "tslib": "^2.7.0",
    "zone.js": "^0.15.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^20.0.0",
    "@angular-devkit/build-angular": "^20.2.2",
    "@angular/build": "^20.0.3",
    "@angular/cli": "^20.2.2",
    "@angular/compiler-cli": "^20.0.3",
    "@types/angular": "^1.8.9",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "jasmine-core": "~5.7.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.2"
  }
}
